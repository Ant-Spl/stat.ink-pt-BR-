{
    "name": "jp3cki/statink",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2.0",
        "ext-PDO": "*",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-filter": "*",
        "ext-gd": "*",
        "ext-hash": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "ext-pdo_pgsql": "*",
        "ext-session": "*",
        "ext-zlib": "*",
        "cweagans/composer-patches": "^1.7.3",
        "geoip2/geoip2": "^2.13",
        "jaybizzle/crawler-detect": "^1.2.116",
        "jp3cki/mb_str_replace": "^4.0.2",
        "jp3cki/uuid": "^3.2.2",
        "jp3cki/yii2-bs-datetimepicker": "^1.0",
        "jp3cki/yii2-flot": "^1.0",
        "jp3cki/yii2-jquery-color": "^1.0",
        "jp3cki/yii2-momentjs": "^1.0.1",
        "jp3cki/yii2-twitter-widget": "^1.0",
        "jp3cki/yii2-zxcvbn": "^1.0",
        "laminas/laminas-feed": "^2.22.0",
        "laminas/laminas-http": "^2.19",
        "laminas/laminas-validator": "^2.43",
        "lipis/flag-icons": "^7.0.2",
        "lusitanian/oauth": "^0.8.11",
        "markrogoyski/math-php": "^2.8.1",
        "omnilight/yii2-scheduling": "^1.1.6",
        "paragonie/constant_time_encoding": "^2.6.3",
        "php-curl-class/php-curl-class": "^9.18.2",
        "phpseclib/phpseclib": "^3.0.34",
        "statink/yii2-anonymizer": "^1.0.4",
        "statink/yii2-bukiicons": "^1.0",
        "statink/yii2-cal-heatmap": "^2.1",
        "statink/yii2-ip-badge": "^1.0.4",
        "statink/yii2-jdenticon": "^1.0",
        "statink/yii2-momentjs": "^1.0",
        "statink/yii2-sortable-table": "^1.0.2",
        "statink/yii2-spl1-stages": "^1.0",
        "statink/yii2-spl2-stages": "^1.0.1",
        "statink/yii2-twitter-webintents": "^1.0.2",
        "symfony/yaml": "^6.4.0|^7.0.0",
        "tpyo/amazon-s3-php-class": "^0.5.1",
        "twbs/bootstrap-icons": "^1.11.2",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-httpclient": "*",
        "yiisoft/yii2-queue": "*",
        "yiisoft/yii2-symfonymailer": "*"
    },
    "require-dev": {
        "codeception/codeception": "*",
        "codeception/module-asserts": "*",
        "codeception/module-filesystem": "*",
        "codeception/module-yii2": "*",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "jp3cki/coding-standard": "^1.0.0",
        "proget-hq/phpstan-yii2": "^0.8.0",
        "squizlabs/php_codesniffer": "^3.7.2",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*"
    },
    "replace": {
        "arokettu/random-polyfill": "*",
        "bower-asset/bootstrap": "^3.4.1",
        "bower-asset/eonasdan-bootstrap-datetimepicker": "*",
        "bower-asset/flot": "~0.8.3",
        "bower-asset/inputmask": "~3.3.5",
        "bower-asset/jquery": "*",
        "bower-asset/jquery-color": "*",
        "bower-asset/moment": "*",
        "bower-asset/moment-timezone": "*",
        "bower-asset/punycode": "1.3.*",
        "bower-asset/yii2-pjax": "*",
        "bower-asset/zxcvbn": "*",
        "npm-asset/bootstrap-notify": "^3.1.3",
        "npm-asset/bootswatch": "^3.4.1",
        "npm-asset/cal-heatmap": "*",
        "npm-asset/dseg": "*",
        "npm-asset/emojify.js": "*",
        "npm-asset/flag-icon-css": "*",
        "npm-asset/flag-icons": "*",
        "npm-asset/jdenticon": "*",
        "npm-asset/jquery": "*",
        "npm-asset/jquery-smooth-scroll": "*",
        "npm-asset/moment": "*",
        "npm-asset/moment-timezone": "*",
        "npm-asset/photoswipe": "*",
        "npm-asset/photoswipe-simplify": "*",
        "npm-asset/popperjs--core": "*",
        "npm-asset/rpg-awesome": "*",
        "npm-asset/stupid-table-plugin": "*",
        "symfony/polyfill-php54": "*",
        "symfony/polyfill-php55": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "config": {
        "process-timeout": 1800,
        "platform": {
            "php": "8.2.13"
        },
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "yiisoft/yii2-composer": true
        }
    },
    "extra": {
        "patches": {
            "lusitanian/oauth": {
                "array_walk interface for PHP 8": "data/patch/lusitanian-oauth-array_walk.patch"
            },
            "tpyo/amazon-s3-php-class": {
                "make static S3::setEndpoint": "data/patch/tpyo-s3-setendpoint-static.patch"
            },
            "yiisoft/yii2": {
                "enhance: callable filter column to GridView": "data/patch/yii2-gridview-callable-filter.patch",
                "enhance: make `protected` CacheController::findCaches()": "data/patch/yii2-cache-command.patch",
                "enhance: use sha256 to caching key": "data/patch/yii2-cache-key.patch"
            },
            "yiisoft/yii2-bootstrap": {
                "non-progress progressbar": "data/patch/yii2-bootstrap-non-progress-progressbar.patch"
            }
        }
    },
    "scripts": {
      "post-install-cmd": [
        "bin/composer-postinstall.sh"
      ],
      "post-update-cmd": [
        "bin/composer-postinstall.sh"
      ]
    },
    "autoload": {
        "psr-4": {
            "app\\": "."
        }
    },
    "repositories": {
        "private": {
            "type": "composer",
            "url": "https://satis.stat.ink",
            "exclude": [
                "statink/yii2-cal-heatmap",
                "statink/yii2-paintball"
            ]
        },
        "heatmap": {
            "type": "vcs",
            "url": "https://github.com/statink/yii2-cal-heatmap.git"
        }
    }
}
